{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "kunaal/footnotes-section",
    "version": "1.0.0",
    "title": "Footnotes Section",
    "category": "kunaal-editorial",
    "icon": "editor-ol-rtl",
    "description": "Renders all footnotes at the bottom of the article. Place this where you want footnotes to appear.",
    "keywords": ["footnotes", "references", "notes", "endnotes"],
    "textdomain": "kunaal-theme",
    "attributes": {
        "title": {
            "type": "string",
            "default": "Notes"
        }
    },
    "supports": {
        "html": false,
        "anchor": true,
        "className": true
    },
    "editorScript": "kunaal-footnotes-section-editor",
    "render": "file:./render.php",
    "style": "file:./style.css"
}

