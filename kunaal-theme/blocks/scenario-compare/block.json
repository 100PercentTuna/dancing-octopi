{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "kunaal/scenario-compare",
    "version": "1.0.0",
    "title": "Scenario Comparison",
    "category": "kunaal-analysis",
    "icon": "columns",
    "description": "Compare multiple scenarios or outcomes side by side.",
    "keywords": ["scenario", "compare", "sensitivity", "outcomes", "options"],
    "textdomain": "kunaal-theme",
    "attributes": {
        "title": {
            "type": "string",
            "default": ""
        },
        "scenarios": {
            "type": "array",
            "default": []
        }
    },
    "supports": {
        "html": false,
        "anchor": true,
        "className": true
    },
    "editorScript": "kunaal-scenario-compare-editor",
    "render": "file:./render.php",
    "style": "file:./style.css"
}

